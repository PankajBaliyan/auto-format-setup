{
  "name": "autoformat_setup",
  "version": "1.0.0",
  "description": "",
  "main": "three.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.html\" && prettier --write \"**/*.js\" && prettier --write \"**/*.css\" && prettier --write \"**/*.jsx\" && npx prettier --write \"**/*.json\" && npx prettier --write \"**/*.md\"",
    "pre-commit": "npm run format",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "*.js": ["eslint --fix"],
    "*.json": ["prettier --write"],
    "*.md": ["prettier --write"]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "stylelint": "^15.7.0",
    "stylelint-config-standard": "^33.0.0"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
